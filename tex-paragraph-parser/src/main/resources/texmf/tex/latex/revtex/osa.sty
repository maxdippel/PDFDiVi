   \ifdim \@tempskipa>\z@
     \begingroup #4\@hangfrom{\hskip #1}{\interlinepenalty \@M
     \ifappendixon
       \if@mainhead %
         \stepcounter{section}%
         \def\@tempa{#5}%
         \ifx\@tempa\empty %
           Appendix%
         \else %
           Appendix:
         \fi
       \fi
     \fi
     #5}\endgroup
   \else \def\@svsechd{#4\hskip #1\relax
     \ifappendixon
       \if@mainhead %
